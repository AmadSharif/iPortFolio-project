@page
@model iPortfolio.Pages.myadmin.AddServiceModel
@{
}
<div class="container">
    <form method="post">
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <h1>Add Services</h1>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Title</label>
                <input class="form-control" asp-for="service.Title">
                <span asp-validation-for="service.Title" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Description</label>
                <input class="form-control" asp-for="service.Desc">
                <span asp-validation-for="service.Desc" class="text-danger"></span>

            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">IconName</label>
                <input class="form-control" asp-for="service.IconName">
                <span asp-validation-for="service.IconName" class="text-danger"></span>

            </div>
        </div>
       

        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add Service" class="btn btn-primary">
            </div>
        </div>
    </form>
</div>
