@page
@model iPortfolio.Pages.myadmin.AddEducationModel
@{
}
<div class="container">
    <form method="post">
        <div class="row  justify-content-center">
            <div class="col-md-4">
               <h1>Add Education</h1>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Degree</label>
                <input class="form-control" asp-for="education.Degree">
                <span asp-validation-for="education.Degree" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Start Year</label>
                <input class="form-control" asp-for="education.StartYear">
                <span asp-validation-for="education.StartYear" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">End Year</label>
                <input class="form-control"asp-for="education.EndYear">
                <span asp-validation-for="education.EndYear" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Institute Name</label>
                <input class="form-control"asp-for="education.InstituteName">
                <span asp-validation-for="education.InstituteName" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Description</label>
                <input class="form-control" asp-for="education.Desc">
                <span asp-validation-for="education.Desc" class="text-danger"></span>
            </div>
        </div>
        <div class="row  justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add Education" class="btn btn-primary">
            </div>
        </div>
    </form>
</div>
